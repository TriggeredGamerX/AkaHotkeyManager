{
  "schemaVersion": 1,
  "id": "aka_hotkeys",
  "version": "${version}",
  "name": "AkaTriggered HotkeysManager",
  "description": "Adds useful QOL hotkeys, such as an elytra swap hotkey and quick totem equip hotkey.",
  "authors": ["AkaTriggered"],
  "contact": {
    "repo": "https://github.com/TriggeredGamerX/AkaHotkeyManager",
    "homepage": "https://modrinth.com/mod/aka-hotkey-manager",
    "sources": "https://github.com/TriggeredGamerX/AkaHotkeyManager",
    "issues": "https://github.com/TriggeredGamerX/AkaHotkeyManager/issues"
  },
  "license": "LGPL-3.0",
  "icon": "assets/aka_hotkey_manager/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.akatriggered.aka_hotkey_manager.client.AkaHotkeysManagerClient"
    ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "1.20.*",
    "java": ">=17"
  }
}
